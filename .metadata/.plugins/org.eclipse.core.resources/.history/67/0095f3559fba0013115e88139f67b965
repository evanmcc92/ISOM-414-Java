import javax.swing.*;


public class electricgenerators extends JApplet{
	public void init(){
		double present_prod, yearly_growth_rate, growth_years, total_growth;
		int counter1 = 1;
		while (counter1 <= 5){
			present_prod = 15000;
			yearly_growth_rate = .03;
			growth_years = 5;
			total_growth = yearly_growth_rate * growth_years;
		}
	}
}